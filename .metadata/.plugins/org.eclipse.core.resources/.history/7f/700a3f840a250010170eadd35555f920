package gioco;

public enum TipoConnettore {
	NONE, SINGOLO, DOPPIO, UNIVERSALE;
	
	public boolean Compatibile(TipoConnettore altro_lato) {
		
		switch(this) {
		case SINGOLO: case DOPPIO:
			return this != NONE && this == altro_lato;
		case UNIVERSALE:
			return true;
		case NONE:
			return false;
		}
		
			
	}

}
